# Google OAuth 2.0 설정
# Google Cloud Console에서 OAuth 2.0 클라이언트 ID 생성 후 입력
GOOGLE_CLIENT_ID=your-google-client-id-here.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=GOCSPX-your-google-client-secret-here

# Notion API 설정
# Notion 개발자 페이지에서 통합 생성 후 토큰 입력
NOTION_TOKEN=secret_AbCdEfGhIjKlMnOpQrStUvWxYz123456789012345678901234567890
NOTION_DATABASE_ID=12345678-1234-1234-1234-123456789012

# 개발 환경 설정
NODE_ENV=development
DEBUG=true

# Chrome Extension 설정
# Chrome 웹 스토어 개발자 대시보드에서 확인 가능 (영문 소문자 32자)
EXTENSION_ID=abcdefghijklmnopqrstuvwxyzabcdef

# API 설정 (선택사항 - OAuth 사용 시 불필요)
GOOGLE_CALENDAR_API_KEY=AIzaSyAbCdEfGhIjKlMnOpQrStUvWxYz123456789
GOOGLE_DRIVE_API_KEY=AIzaSyAbCdEfGhIjKlMnOpQrStUvWxYz123456789
GOOGLE_SHEETS_API_KEY=AIzaSyAbCdEfGhIjKlMnOpQrStUvWxYz123456789